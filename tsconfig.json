{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "esnext"
    ],
    "moduleResolution": "bundler",
    "types": [
      "react",
      "react-native"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}